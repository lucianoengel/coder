{
  "models": {
    "gemini": {
      "model": "gemini-3.1-pro-preview",
      "apiEndpoint": "https://generativelanguage.googleapis.com/v1beta",
      "apiKeyEnv": "GEMINI_API_KEY"
    },
    "claude": {
      "model": "claude-sonnet-4-6",
      "apiEndpoint": "https://api.anthropic.com",
      "apiKeyEnv": "ANTHROPIC_API_KEY"
    },
    "codex": {
      "model": "gpt-5.3-codex",
      "apiEndpoint": "https://api.openai.com",
      "apiKeyEnv": "OPENAI_API_KEY"
    }
  },
  "workflow": {
    "agentRoles": {
      "issueSelector": "gemini",
      "planner": "claude",
      "planReviewer": "gemini",
      "programmer": "claude",
      "reviewer": "codex",
      "committer": "codex"
    },
    "wip": {
      "push": true,
      "autoCommit": true,
      "includeUntracked": false,
      "remote": "origin",
      "failOnError": false
    },
    "scratchpad": {
      "sqliteSync": true,
      "sqlitePath": ".coder/state.db"
    },
    "localIssuesDir": ""
  },
  "ppcommit": {
    "preset": "strict",
    "enableLlm": true,
    "llmModelRef": "gemini"
  },
  "test": {
    "command": "",
    "allowNoTests": false
  },
  "design": {
    "stitch": {
      "enabled": false,
      "transport": "stdio",
      "serverCommand": "",
      "serverUrl": "",
      "apiKeyEnv": "GOOGLE_STITCH_API_KEY",
      "authHeader": "X-Goog-Api-Key"
    },
    "specDir": "spec/UI"
  },
  "github": {
    "epicAsMilestone": true,
    "defaultLabels": []
  }
}
